{
    "$schema": "../../tools/src/prebuilds/schemas/spm.config.schema.json",
    "products": [
        {
            "name": "EXApplication",
            "platforms": [
                "iOS(.v15)"
            ],
            "externalDependencies": [
                "ReactNativeDependencies",
                "React",
                "Hermes",
                "expo-modules-core/ExpoModulesCore"
            ],
            "targets": [
                {
                    "type": "objc",
                    "name": "EXApplication_ios_objc",
                    "moduleName": "EXApplication",
                    "path": "ios/EXApplication",
                    "pattern": "**/*.m",
                    "headerPattern": "**/*.h",
                    "dependencies": [
                        "Hermes",
                        "React",
                        "ReactNativeDependencies"
                    ],
                    "linkedFrameworks": [ "Foundation", "UIKit" ],
                    "includeDirectories": [ "." ]
                },
                {
                    "type": "swift",
                    "name": "EXApplication",
                    "path": "ios",
                    "pattern": "*.swift",
                    "dependencies": [
                        "Hermes",
                        "React",
                        "ReactNativeDependencies",
                        "expo-modules-core/ExpoModulesCore",
                        "EXApplication_ios_objc"
                    ],
                    "linkedFrameworks": [ "Foundation", "UIKit" ]
                }
            ]
        }
    ]
}
