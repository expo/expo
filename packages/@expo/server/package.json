{
  "name": "@expo/server",
  "version": "0.7.2",
  "description": "Server API for Expo Router projects",
  "main": "build/cjs/index.js",
  "module": "build/mjs/index.js",
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/cjs/index.d.ts"
    },
    "./adapter/*": {
      "import": "./build/mjs/vendor/*.js",
      "require": "./build/cjs/vendor/*.js",
      "types": "./build/cjs/vendor/*.d.ts"
    },
    "./build/*": {
      "import": "./build/mjs/*.js",
      "require": "./build/cjs/*.js",
      "types": "./build/cjs/*.d.ts"
    }
  },
  "scripts": {
    "build": "yarn run build:cjs && yarn run build:mjs",
    "build:cjs": "expo-module build",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "clean": "expo-module clean",
    "prepare": "yarn run clean && yarn build",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/@expo/server"
  },
  "keywords": [
    "expo"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/expo/tree/main/packages/@expo/server#readme",
  "files": [
    "build",
    "adapter"
  ],
  "dependencies": {
    "abort-controller": "^3.0.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@netlify/functions": "^1.4.0",
    "@types/express": "^5.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
