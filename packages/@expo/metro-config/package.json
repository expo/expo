{
  "name": "@expo/metro-config",
  "version": "0.15.0",
  "description": "A Metro config for running React Native projects with the Metro bundler",
  "main": "build/ExpoMetroConfig.js",
  "scripts": {
    "build": "tsc --emitDeclarationOnly && babel src --out-dir build --extensions \".ts\" --source-maps --ignore \"src/**/__mocks__/*\",\"src/**/__tests__/*\"",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "prepare": "expo-module clean && yarn run build",
    "prepublishOnly": "expo-module prepublishOnly",
    "test": "expo-module test",
    "typecheck": "expo-module typecheck",
    "watch": "expo-module build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/@expo/metro-config"
  },
  "keywords": [
    "expo",
    "metro"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/expo/tree/main/packages/@expo/metro-config#readme",
  "files": [
    "build",
    "transformer",
    "file-store",
    "babel-transformer"
  ],
  "dependencies": {
    "@expo/config": "~8.4.0",
    "@expo/env": "~0.1.0",
    "@expo/json-file": "~8.2.37",
    "@react-native/metro-config": "~0.73.2",
    "babel-preset-fbjs": "^3.4.0",
    "chalk": "^4.1.0",
    "debug": "^4.3.2",
    "find-yarn-workspace-root": "~2.0.0",
    "getenv": "^1.0.0",
    "jsc-safe-url": "^0.2.4",
    "lightningcss": "~1.19.0",
    "path-to-regexp": "~1.8.0",
    "postcss": "~8.4.21",
    "resolve-from": "^5.0.0",
    "sucrase": "^3.20.0"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@react-native/babel-preset": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@types/path-to-regexp": "^1.7.0",
    "expo-module-scripts": "^3.3.0",
    "sass": "^1.60.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
