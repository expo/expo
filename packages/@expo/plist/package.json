  "homepage": "https://github.com/expo/expo/tree/main/packages/@expo/plist#readme",
  "files": [
    "build"
  ],
  "dependencies": {
    "@xmldom/xmldom": "~0.7.7",
    "base64-js": "^1.2.3",
    "xmlbuilder": "^14.0.0"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
