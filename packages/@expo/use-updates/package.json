{
  "name": "@expo/use-updates",
  "version": "0.1.0",
  "description": "An easier to use, declarative JavaScript API for the expo-updates module.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "prepare": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts; expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "keywords": [
    "react-native",
    "expo",
    "updates"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/@expo/use-updates"
  },
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "author": "650 Industries, Inc.",
  "license": "MIT",
  "homepage": "https://docs.expo.dev/versions/latest/sdk/updates/",
  "jest": {
    "preset": "expo-module-scripts",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-native": "^11.3.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^17.0.15",
    "@types/node-forge": "^1.0.0",
    "expo-module-scripts": "^3.0.0",
    "express": "^4.17.2",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "memfs": "^3.2.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "peerDependencies": {
    "expo": "*",
    "expo-updates": "*"
  }
