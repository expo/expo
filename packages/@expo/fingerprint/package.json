{
  "name": "@expo/fingerprint",
  "version": "0.0.5",
  "description": "A library to generate a fingerprint from a React Native project",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc --sourceMap",
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf build ./tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "test": "jest",
    "test:e2e": "jest --config e2e/jest.config.js"
  },
  "bin": {
    "fingerprint": "bin/cli.js"
  },
  "keywords": [
    "expo",
    "react-native",
    "fingerprint",
    "hash",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/@expo/fingerprint"
  },
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "author": "650 Industries, Inc.",
  "license": "MIT",
  "homepage": "https://github.com/expo/expo/tree/main/packages/@expo/fingerprint#readme",
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "find-up": "^5.0.0",
    "minimatch": "^3.0.4",
    "p-limit": "^3.1.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@types/find-up": "^4.0.0",
    "@types/jest": "^29.2.1",
    "@types/rimraf": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-universe": "^11.1.0",
    "expo": "^48.0.0-beta.0",
    "glob": "^7.1.7",
    "jest": "^29.2.1",
    "jest-watch-typeahead": "2.2.1",
    "memfs": "^3.4.12",
    "rimraf": "^3.0.2",
    "temp-dir": "^2.0.0",
    "typescript": "^4.3.5"
  }
}
