{
  "name": "eslint-config-universe",
  "version": "4.0.0",
  "description": "Shared ESLint configs for universal Expo projects, Node, and web",
  "main": "default.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo.git",
    "directory": "packages/eslint-config-universe"
  },
  "keywords": [
    "eslint-config",
    "expo",
    "react-native"
  ],
  "files": [
    "README.md",
    "default.js",
    "native.js",
    "node.js",
    "web.js",
    "shared"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*-test.js"
    ]
  },
  "eslintIgnore": [
    "/__tests__/fixtures/*"
  ],
  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/expo/tree/master/packages/eslint-config-universe",
  "dependencies": {
    "@babel/eslint-plugin": "^7.11.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0"
  },
  "peerDependencies": {
    "eslint": ">= 6.0.0",
    "prettier": ">= 1.19.0"
  },
  "devDependencies": {
    "@expo/spawn-async": "^1.5.0",
    "eslint": "^7.9.0",
    "jest": "^26.4.1",
    "prettier": "^1.19.1"
  }
}
