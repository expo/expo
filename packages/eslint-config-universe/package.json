{
  "name": "eslint-config-universe",
  "version": "10.0.0",
  "description": "Shared ESLint configs for universal Expo projects, Node, and web",
  "main": "default.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo.git",
    "directory": "packages/eslint-config-universe"
  },
  "keywords": [
    "eslint-config",
    "expo",
    "react-native"
  ],
  "files": [
    "README.md",
    "default.js",
    "native.js",
    "node.js",
    "web.js",
    "shared"
  ],
  "jest": {
    "testMatch": [
      "**/__tests__/*-test.js"
    ]
  },
  "eslintIgnore": [
    "/__tests__/fixtures/*"
  ],
  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/expo/tree/main/packages/eslint-config-universe",
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "peerDependencies": {
    "@babel/core": ">=7.16",
    "eslint": ">=7.28",
    "prettier": ">=2.4"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@expo/spawn-async": "^1.6.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1"
  }
}
