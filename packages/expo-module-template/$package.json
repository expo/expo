{
  "name": "<%- project.slug %>",
  "version": "<%- project.version %>",
  "description": "<%- project.description %>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"<% const platformScripts = [];
      if (features.ios) platformScripts.push(`${JSON.stringify('open:ios')}: ${JSON.stringify('xed example/ios')}`);
      if (features.android) platformScripts.push(`${JSON.stringify('open:android')}: ${JSON.stringify('open -a "Android Studio" example/android')}`);
      if (platformScripts.length) { %>,
    <%- platformScripts.join(',\n    ') %><% } %>
  },
  "keywords": ["react-native", "expo", "<%- project.slug %>", "<%- project.name %>"],
  "repository": "<%- repo %>",
  "bugs": {
    "url": "<%- repo %>/issues"
  },
  "author": "<%- author %>",
  "license": "<%- license %>",
  "homepage": "<%- repo %>#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "~19.1.1",
    "expo-module-scripts": "^5.0.7",
    "expo": "^54.0.8",
    "react-native": "0.82.1"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
