{
  "name": "expo-module-scripts",
  "version": "5.0.7",
  "description": "A private package for various tasks for Expo module packages like compiling and testing",
  "main": "index.js",
  "type": "module",
  "bin": {
    "expo-module": "bin/expo-module.js"
  },
  "exports": {
    "./package.json": "./package.json",
    "./babel.config.*": {
      "require": "./babel.config.*.js"
    },
    "./babel.config.*.js": {
      "require": "./babel.config.*.js"
    },
    "./eslintrc.base.js": {
      "require": "./eslintrc.base.cjs"
    },
    "./eslint.config.base": {
      "require": "./eslint.config.base.cjs"
    },
    "./jest-preset-*": {
      "import": "./jest-preset-*.cjs",
      "require": "./jest-preset-*.cjs"
    },
    "./jest-preset-*.js": {
      "import": "./jest-preset-*.cjs",
      "require": "./jest-preset-*.cjs"
    },
    "./jest-preset": {
      "import": "./jest-preset.cjs",
      "require": "./jest-preset.cjs"
    },
    "./jest-preset.js": {
      "import": "./jest-preset.cjs",
      "require": "./jest-preset.cjs"
    },
    "./*/jest-preset": {
      "import": "./*/jest-preset.cjs",
      "require": "./*/jest-preset.cjs"
    },
    "./*/jest-preset.js": {
      "import": "./*/jest-preset.cjs",
      "require": "./*/jest-preset.cjs"
    },
    "./tsconfig.*": "./tsconfig.*.json",
    "./tsconfig": "./tsconfig.json",
    "./types/*.d.ts": {
      "types": "./types/*.d.ts"
    }
  },
  "scripts": {
    "lint": "expo-module eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo.git",
    "directory": "packages/expo-module-scripts"
  },
  "keywords": [
    "babel-preset",
    "jest-preset",
    "expo"
  ],
  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/expo/tree/main/packages/expo-module-scripts#readme",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@expo/metro": "~54.1.0",
    "@expo/npm-proofread": "^1.0.1",
    "@expo/spawn-async": "^1.7.2",
    "@testing-library/react-native": "^13.2.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.2.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-preset-expo": "~54.0.1",
    "commander": "^12.1.0",
    "eslint-config-universe": "^15.0.3",
    "glob": "^13.0.0",
    "jest-expo": "~54.0.11",
    "jest-snapshot-prettier": "npm:prettier@^2",
    "jest-watch-typeahead": "2.2.1",
    "resolve-workspace-root": "^2.0.0",
    "ts-jest": "~29.0.4",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0"
  }
}
