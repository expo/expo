{
  "name": "expo-doctor",
  "version": "1.17.7",
  "main": "build/index.js",
  "description": "Check your Expo project for known issues",
  "keywords": [
    "expo",
    "ios",
    "expo-doctor",
    "android"
  ],
  "homepage": "https://docs.expo.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/expo-doctor"
  },
  "author": "Expo <support@expo.dev>",
  "license": "MIT",
  "bin": "./build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "prepare": "pnpm run clean && pnpm run build:prod",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "typecheck": "expo-module typecheck",
    "test": "expo-module test",
    "watch": "pnpm run build -w",
    "prepublishOnly": "expo-module prepublishOnly"
  },
  "devDependencies": {
    "@expo/cli": "workspace:*",
    "@expo/config": "workspace:*",
    "@expo/env": "workspace:*",
    "@expo/json-file": "workspace:*",
    "@expo/metro": "~54.1.0",
    "@expo/schemer": "workspace:*",
    "@expo/spawn-async": "^1.7.2",
    "@types/debug": "^4.1.8",
    "@types/getenv": "^1.0.0",
    "@types/semver": "^7.5.8",
    "@vercel/ncc": "0.38.3",
    "expo-modules-autolinking": "workspace:*",
    "expo": "workspace:*",
    "chalk": "^4.0.0",
    "debug": "^4.3.4",
    "expo-module-scripts": "workspace:*",
    "getenv": "^2.0.0",
    "glob": "^13.0.0",
    "ignore": "^5.3.2",
    "ora": "3.4.0",
    "resolve-from": "^5.0.0",
    "resolve-workspace-root": "^2.0.0",
    "semver": "7.5.4",
    "terminal-link": "^2.1.1"
  }
}
