{
  "name": "create-expo",
  "version": "2.1.2",
  "bin": "./build/index.js",
  "main": "build",
  "description": "Create universal Expo apps",
  "license": "BSD-3-Clause",
  "keywords": [
    "expo",
    "react-native",
    "react"
  ],
  "homepage": "https://docs.expo.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/create-expo"
  },
  "author": "Evan Bacon <bacon@expo.io> (https://github.com/evanbacon)",
  "files": [
    "build",
    "template"
  ],
  "scripts": {
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "prepare": "expo-module clean && yarn run build:prod",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "typecheck": "expo-module typecheck",
    "test": "expo-module test",
    "test:e2e": "expo-module test --config e2e/jest.config.js",
    "watch": "yarn run build --watch",
    "prepublishOnly": "expo-module prepublishOnly"
  },
  "devDependencies": {
    "@expo/json-file": "8.2.37",
    "@expo/package-manager": "^1.1.1",
    "@expo/spawn-async": "^1.7.0",
    "@types/debug": "^4.1.7",
    "@types/getenv": "^1.0.0",
    "@types/node": "^16.11.56",
    "@types/node-fetch": "^2.5.8",
    "@types/prompts": "2.0.14",
    "@types/tar": "^6.1.2",
    "@vercel/ncc": "^0.38.0",
    "arg": "^5.0.2",
    "chalk": "^4.0.0",
    "debug": "^4.3.4",
    "expo-module-scripts": "^3.0.0",
    "getenv": "^1.0.0",
    "minipass": "^3.3.6",
    "node-fetch": "^2.6.7",
    "ora": "3.4.0",
    "prompts": "^2.4.2",
    "tar": "6.1.13",
    "update-check": "^1.5.4"
  }
}
