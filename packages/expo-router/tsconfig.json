{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "module": "commonjs"
  },
  "include": [
    "./src",
    "./types/expo-router.d.ts",
    "./types/expect.d.ts",
    "../expo/src/ts-declarations"
  ],
  "exclude": [
    "**/__mocks__/*",
    "**/.expo/*",
    "./build/*",
    "**/__tests__/*",
    "**/__typetests__/*",
    "./node",
    "./entry.js",
    "./_ctx.d.ts"
  ]
}