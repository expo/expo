{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "module": "commonjs",
    "types": [
      "jest",
      "jest-require",
      "node"
    ]
  },
  "include": [
    "./src",
    "./types/expect.d.ts",
    "./types/react-navigation.d.ts",
    "../@expo/cli/ts-declarations",
    "../expo/src/ts-declarations"
  ],
  "exclude": [
    "**/__mocks__/*",
    "**/.expo/*",
    "./build/*",
    "**/__tests__/*",
    "**/__rsc_tests__/*",
    "./node",
    "./entry.js",
    "./_ctx.d.ts"
  ]
}