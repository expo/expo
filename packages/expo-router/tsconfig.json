{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "module": "commonjs",
    "moduleResolution": "node10",
    "noEmit": false,
    "resolveJsonModule": true,
    "types": ["node", "jest"]
  },
  "include": ["./src"],
  "exclude": [
    "**/__tests__/",
    "**/__rsc_tests__/"
  ]
}
