{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "**/__tests__/",
    "**/__rsc_tests__/",
    "./types/expect.d.ts",
    "./src/ts-declarations",
    "../@expo/cli/ts-declarations",
    "../expo/src/ts-declarations"
  ],
  "exclude": [
    "**/__mocks__/*",
    "**/.expo/*",
    "**/typed-routes/__tests__/*",
    "./build/*",
    "./node",
    "./entry.js",
    "./_ctx.d.ts"
  ]
}
