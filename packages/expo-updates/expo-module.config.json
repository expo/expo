  "ios": {
    "modules": ["UpdatesModule"],
    "appDelegateSubscribers": ["ExpoUpdatesAppDelegateSubscriber"],
    "reactDelegateHandlers": ["ExpoUpdatesReactDelegateHandler"]
  },
  "android": {
    "gradlePlugins": [
      {
        "id": "expo-updates-gradle-plugin",
