{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@expo/vector-icons": ["react-native-vector-icons"],
      "ThemeProvider": ["../common/ThemeProvider"],
      "benchmarking": ["../bare-expo/modules/benchmarking"]
    },
    "types": ["jest"],
    "typeRoots": ["./ts-declarations", "./node_modules/@types", "../../node_modules/@types"],
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "rootDirs": ["./", "../bare-expo/.expo/localModules/types"]
  },
  "include": [
    "App.tsx",
    "src/**/*",
    "ts-declarations/**/*",
    "../bare-expo/.expo/localModules/types/**/*"
  ]
}
