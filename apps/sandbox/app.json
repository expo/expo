{
  "expo": {
    "name": "sandbox",
    "slug": "sandbox",
    "sdkVersion": "UNVERSIONED",
    "scheme": "sandbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://naviloop.netlify.app/",
          "asyncRoutes": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "output": "server"
    }
  }
}
