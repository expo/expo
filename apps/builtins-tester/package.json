{
  "name": "@expo/builtins-tester",
  "version": "0.0.0",
  "main": "./index",
  "scripts": {
    "prebuild": "TEMPLATE_DIR=\"../../templates/expo-template-bare-minimum\" && cd \"$TEMPLATE_DIR\" && PACKED_FILE=$(npm pack) && mv \"$PACKED_FILE\" pack.tgz && cd - && npx expo prebuild --clean --template \"$TEMPLATE_DIR/pack.tgz\"",
    "start": "EXPO_ATLAS=1 expo start",
    "android": "expo run:android",
    "ios": "yarn run prepare-builtins && expo run:ios",
    "prepare-builtins": "cd ../builtins && yarn run bundle"
  },
  "dependencies": {
    "expo": "~54.0.8",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "*",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.6",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "4.19.0-nightly-20251203-1746a584e"
  },
  "ext": {
    "@expo/vector-icons": "*",
    "@react-navigation/bottom-tabs": "*",
    "@react-navigation/elements": "*",
    "@react-navigation/native": "*",
    "expo-blur": "*",
    "expo-constants": "*",
    "expo-font": "*",
    "expo-haptics": "*",
    "expo-image": "*",
    "expo-linking": "*",
    "expo-router": "*",
    "expo-symbols": "*",
    "expo-system-ui": "*",
    "expo-web-browser": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*",
    "react-native-safe-area-context": "*",
    "react-native-screens": "*",
    "react-native-webview": "*"
  },
  "private": true
}
