{
  "name": "native-tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "expo-yarn-workspaces postinstall",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "~47.0.0-alpha.1",
    "expo-camera": "~13.1.0",
    "expo-dev-client": "~2.0.0",
    "expo-error-recovery": "~4.0.0",
    "expo-face-detector": "~12.0.0",
    "expo-image": "~0.2.0",
    "expo-notifications": "~0.17.0",
    "expo-random": "~13.0.0",
    "expo-splash-screen": "~0.17.0",
    "native-component-list": "*",
    "react": "18.1.0",
    "react-dom": "18.0.0",
    "react-native": "0.70.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  },
  "expo-yarn-workspaces": {
    "symlinks": [
      "expo-constants",
      "expo-camera",
      "expo-updates",
      "react-native-reanimated"
    ]
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-branch",
        "expo-ads-facebook",
        "expo-ads-admob",
        "expo-apple-authentication",
        "expo-module-template"
      ],
      "ios": {
        "flags": {
          "inhibit_warnings": false
        }
      }
    }
  },
  "private": true
}
