{
  "name": "@expo/image-comparison",
  "version": "1.0.0",
  "private": true,
  "description": "Image comparison server and CLI for E2E testing",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
		"buildInspector": "./inspector/scripts/build.sh",
    "dev": "bun --watch --no-clear-screen ./src/server.ts",
    "start": "bun run src/server.ts",
    "test": "bun test",
    "compare": "bun run src/compareImages.ts",
    "debug-viewshot": "bun run src/debug-viewshot.ts",
    "debug-inspector": "bun run inspector/ScreenInspectorIOS.ts"
  },
  "dependencies": {
    "jimp-compact": "^0.16.1-2",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pngjs": "^6.0.5"
  }
}
