    "@mdx-js/loader": "^2.2.1",
    "@mdx-js/mdx": "^2.2.1",
    "@mdx-js/react": "^2.2.1",
    "@reach/tabs": "^0.17.0",
    "@sentry/react": "^7.24.1",
    "@sentry/tracing": "^7.24.1",
    "cmdk": "^0.2.0",
    "date-fns": "^2.29.3",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.1.0",
    "github-slugger": "^1.3.0",
    "lodash": "^4.17.21",
    "next": "^13.3.1",
    "nprogress": "0.2.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "1.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-diff-view": "^2.4.10",
    "react-dom": "^18.2.0",
    "react-grid-system": "^8.1.6",
    "react-markdown": "^8.0.4",
    "react-player": "^1.11.1",
    "react-visibility-sensor": "^5.1.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.2.1",
    "remark-mdx-disable-explicit-jsx": "^0.1.0",
    "remark-mdx-frontmatter": "^2.1.1",
    "tippy.js": "^6.3.7"
