{
  "name": "expo-docs",
  "version": "32.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "export": "yarn run build && cross-env NODE_OPTIONS=--max-old-space-size=8192 next export",
    "export-server": "cd out && python -m SimpleHTTPServer 8000",
    "import-react-native-docs": "node ./scripts/import-react-native-docs.js",
    "test-links": "blc --recursive --exclude-external --filter-level 2"
  },
  "dependencies": {
    "@mdx-js/loader": "^0.16.8",
    "@mdx-js/mdx": "^0.16.8",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-preval": "^3.0.1",
    "cross-env": "^5.2.0",
    "docsearch.js": "^2.5.2",
    "emotion": "^9.2.9",
    "emotion-server": "^9.2.9",
    "front-matter": "^2.3.0",
    "fs-extra": "^6.0.1",
    "github-slugger": "^1.2.0",
    "hotshot": "^1.0.5",
    "next": "^8.0.0",
    "nprogress": "0.2.0",
    "prismjs": "^1.11.0",
    "prop-types": "15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-emotion": "^9.2.9"
  },
  "devDependencies": {
    "broken-link-checker": "^0.7.8",
    "minimist": "^1.2.0",
    "prettier": "^1.15.3"
  }
}
