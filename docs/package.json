{
  "name": "expo-docs",
  "version": "52.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn generate-static-resources && next dev -p 3002",
    "build": "yarn copy-latest && next build",
    "export": "yarn generate-static-resources production && yarn append-last-modified-dates && yarn run build && yarn run export-issue-404",
    "export-preview": "yarn generate-static-resources preview && yarn append-last-modified-dates && yarn run build && yarn run export-issue-404",
    "export-issue-404": "echo \"ðŸ›   Patching https://github.com/vercel/next.js/issues/16528\"; cp out/404/index.html out/404.html",
    "export-server": "http-server out -p 8000",
    "lint": "tsc --noEmit && node scripts/lint.js",
    "lint-links": "remark -u validate-links ./pages",
    "lint-prose": "./node_modules/@vvago/vale/bin/vale .",
    "watch": "tsc --noEmit -w",
    "test": "yarn generate-static-resources && yarn node --experimental-vm-modules $(yarn bin jest)",
    "schema-sync": "node --async-stack-traces --unhandled-rejections=strict ./scripts/schema-sync.cjs",
    "permissions-sync-android": "npx scrape-permissions-json@latest components/plugins/permissions/data --android",
    "remove-version": "node --unhandled-rejections=strict ./scripts/remove-version.js",
    "generate-static-resources": "rimraf public/static/constants && node --unhandled-rejections=strict ./scripts/generate-static-resources.js ./scripts/generate-llm-txt.js ./scripts/generate-llm-full-txt.js",
    "append-last-modified-dates": "node --unhandled-rejections=strict ./scripts/append-dates.js",
    "copy-latest": "node ./scripts/copy-latest.js",
    "postinstall": "yarn copy-latest && yarn generate-static-resources"
  },
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "@expo/styleguide": "^9.0.2",
    "@expo/styleguide-base": "^2.0.1",
    "@expo/styleguide-icons": "^2.1.1",
    "@expo/styleguide-search-ui": "^2.2.5",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-select": "2.1.1",
    "@radix-ui/react-tooltip": "^1.1.1",
    "@reach/tabs": "^0.18.0",
    "@sentry/react": "^8.47.0",
    "clipboard-copy": "^4.0.1",
    "cmdk": "^0.2.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.15.0",
    "front-matter": "^4.0.2",
    "github-slugger": "^2.0.0",
    "lodash": "^4.17.21",
    "next": "^15.1.3",
    "nprogress": "0.2.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-confetti-explosion": "^2.1.2",
    "react-diff-view": "^3.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.2",
    "react-player": "^2.16.0",
    "react-qr-code": "^2.0.15",
    "react-visibility-sensor": "^5.1.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.1.0",
    "remark-mdx-disable-explicit-jsx": "^0.1.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "tippy.js": "^6.3.7",
    "yet-another-react-lightbox": "^3.21.7"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.3",
    "@expo/spawn-async": "^1.7.2",
    "@tailwindcss/typography": "^0.5.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/google.analytics": "^0.0.46",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.14.8",
    "@types/nprogress": "^0.2.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vvago/vale": "^3.9.2",
    "acorn": "^8.14.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.3",
    "eslint-config-universe": "^14.0.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-testing-library": "^6.5.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "fs-extra": "^11.2.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-router-mock": "^0.9.13",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-test-renderer": "^18.3.1",
    "rehype-slug": "^6.0.0",
    "remark-cli": "^12.0.1",
    "remark-supersub": "^1.0.0",
    "remark-validate-links": "^13.0.2",
    "rimraf": "^6.0.1",
    "semver": "^7.6.3",
    "sitemap": "^7.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "user-agent-data-types": "^0.4.2"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
      "mergeClasses"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
