const FayobetApp = {
  utilisateur: {
    estInscrit: false,
    infos: {
      nom: null,
      email: null,
      pays: null,
      monnaie: null,
      codeBonus: null
},
    solde: 0,
    historiqueTransferts: [],
    authentifie2FA: false
},

  politiqueConfidentialite: {
    collecte: "CoordonnÃ©es, localisation, paiements, historique de paris",
    consentement: "Traitement des donnÃ©es avec consentement explicite",
    securite: "Mesures RGPD pour confidentialitÃ© et protection",
    cookies: ["session", "persistents", "analytiques"],
    droitsUtilisateur: [
      "Modification",
      "VÃ©rification",
      "Suppression des donnÃ©es personnelles"
    ],
    afficher: function() {
      console.log("ğŸ” Politique de confidentialitÃ© de Fayobet");
      console.log(`ğŸ“„ Collecte: ${this.collecte}`);
      console.log(`âœ… Consentement: ${this.consentement}`);
      console.log(`ğŸ”’ SÃ©curitÃ©: ${this.securite}`);
      console.log(`ğŸª Cookies utilisÃ©s: ${this.cookies.join(", ")}`);
      console.log(`ğŸ§¾ Droits: ${this.droitsUtilisateur.join(", ")}`);
}
},

  inscrireUtilisateur: function(nom, email, pays, monnaie, codeBonus) {
    console.log("ğŸ“ Inscription en cours...");
    this.utilisateur.infos = { nom, email, pays, monnaie, codeBonus};
    this.utilisateur.estInscrit = true;
    console.log(`âœ… Inscription rÃ©ussie pour ${nom} (${email})`);
    this.activer2FA();
},

  activer2FA: function() {
    this.utilisateur.authentifie2FA = true;
    console.log("ğŸ” Authentification double facteur activÃ©e.");
},

  deposerFonds: function(montant, moyenPaiement) {
    const minimum = 3000;
    if (montant>= minimum) {
      this.utilisateur.solde += montant;
      this.utilisateur.historiqueTransferts.push(`DÃ©pÃ´t de ${montant} via ${moyenPaiement}`);
      console.log(`ğŸ’° DÃ©pÃ´t de ${montant} XOF via ${moyenPaiement} effectuÃ©.`);
} else {
      console.log("â›” Montant de dÃ©pÃ´t insuffisant.");
}
},

  retirerFonds: function(montant, moyenRetrait) {
    const minimumRetrait = 3000;
    if (montant <= this.utilisateur.solde && montant>= minimumRetrait) {
      this.utilisateur.solde -= montant;
      this.utilisateur.historiqueTransferts.push(`Retrait de ${montant} via ${moyenRetrait}`);
      console.log(`ğŸ§ Retrait de ${montant} XOF via ${moyenRetrait} lancÃ©.`);
} else {
      console.log("â›” Retrait impossible: solde insuffisant ou montant trop bas.");
}
}
};

