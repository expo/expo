{
  "extends": "@tsconfig/node18/tsconfig.json",

  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true
  }
}
