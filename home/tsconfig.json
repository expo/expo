{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "resolveJsonModule": true,
    "baseUrl": ".",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["**/__mocks__/*"]
}
